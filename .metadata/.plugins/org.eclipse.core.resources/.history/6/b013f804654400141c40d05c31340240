package spaceTrader.Model;

public class SolarSystem {
	
	
	private int x;
	private int y;
	private Capital planet;
	
	public SolarSystem (int x, int y, Capital planet) {
		setPlanet(planet);
		setX(x);
		setY(y);
	}
	
	
	public Capital getPlanet() {
		return planet;
	}
	public void setPlanet(Capital planet) {
		this.planet = planet;
	}
	public int getX() {
		return x;
	}
	public void setX(int x) {
		this.x = x;
	}
	public int getY() {
		return y;
	}
	public void setY(int y) {
		this.y = y;
	}
	
	




}
